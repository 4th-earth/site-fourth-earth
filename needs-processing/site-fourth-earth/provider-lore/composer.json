{
    "name": "fourth-earth/provider-fourth-earth-lore",
    "version": "0.0.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Bruce",
            "email": "josh@joshbruce.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.1",
        "phpunit/php-code-coverage": "~9"
    },
    "autoload": {
        "psr-4": {
            "FourthEarth\\Lore\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FourthEarth\\Lore\\Tests\\": "./tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FourthEarth\\Lore\\Provider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
