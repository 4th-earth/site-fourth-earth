{
    "name": "joshbruce/fourth-earth",
    "version": "0.0.3",
    "type": "project",
    "description": "Future-fantasy, text-based RPG.",
    "require": {
        "php": "^7.3",
        "8fold/laravel-markup": "~0.0.39",
        "joshbruce/php-dice-tower": "~0.0.4",
        "hashids/hashids": "~4.0",
        "zacksmash/fortify-ui": "~1.3",
        "livewire/livewire": "~2.3",
        "doctrine/dbal": "~2.12"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
          "FourthEarth\\Site\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FourthEarth\\Site\\Tests\\": "./tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FourthEarth\\Site\\Provider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
